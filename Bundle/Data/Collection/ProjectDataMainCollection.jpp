class ProjectDataMainCollection extends NamedDataDataMainCollection {

	_entityType = 'Project';
	_entityClass = 'project_data_main_entity';

	getProtected : function () {
		if (this._protect == undefined) {
			var parent = _super();
			this._protect = parent.concat(
				Config.modules.Project.collections.update.protect
			);
		}
		return this._protect;
	}

}